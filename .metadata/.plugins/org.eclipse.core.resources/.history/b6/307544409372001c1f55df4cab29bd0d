import java.util.Scanner;

public class Assign1 {
	static void q1() {
		int last = 1, slast = 0;
		Scanner sc = new Scanner(System.in);
//		int n = sc.nextInt();
		int n=20;
//		System.out.print(slast + " " + last + " ");
		int sum = 1;
		for (int i = 0; i < n - 1; i++) {
			int curr = last + slast;
			sum += curr;
			System.out.print(curr + " ");
			slast = last;
			last = curr;
		}
		System.out.println("\n" + (float) sum / n);

	}

	public static void main(String[] args) {
		q1();
	}

}
